.navigator {
  display: flex;
  flex-direction: row;
  align-items: flex-start;
  justify-content: flex-start;
}
.itemIcon{
  width: 15%;
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: center;
  padding:20rpx 0rpx 10rpx 15rpx;
  font-family:"icon" !important;
  font-size:80rpx;
  color: #0074d9;
  font-style:normal;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}
.itemContent{
  width: 82%;
  padding: 25rpx 30rpx 15rpx 5rpx;
  position: relative;
  margin-left: 12rpx;
}
.itemContent::before{
  content: "";
  position: absolute;
  left: 10rpx;
  top: 0;
  right: 30rpx;
  height: 2rpx;
  border-top: 1rpx solid #D8D8D8;
  color: #D8D8D8;
}
.itemContent::first-line::before{
  display: none;
}
.itemTitleBox{
  display: flex;
  flex-direction: row;
  align-items: flex-start;
  justify-content: space-between;
}
.startBox{
  width: 70%;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}
.endBox{
  width: auto;
  display: flex;
  flex-direction: row;
  align-items: flex-start;
  justify-content: space-around;
}
.dateBox{
  display: flex;
  align-items: flex-start;
  font-size: 25rpx;
  color: #999999;
}
.itemSubTitleBox{
  font-size: 25rpx;
  color: #999999;
  width: 100%;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}