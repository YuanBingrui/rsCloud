page {
  background-color: #eeeeee;
}
.section{
  padding: 20rpx 0 20rpx 0;
  background-color: #fff;
}
.itemDetailContent{
  margin: 15rpx 0;
}
.itemDetailBox{
  border-top: 1px #D8D8D8 solid;
  border-bottom: 1px #D8D8D8 solid;
}
.oneItemDetail {
  padding: 20rpx 30rpx;
  position: relative;
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: space-between;
}
.oneItemDetail:before {
  content: " ";
  position: absolute;
  left: 30rpx;
  top: 0;
  right: 30rpx;
  height: 1px;
  border-top: 0.5rpx solid #D8D8D8;
  color: #D8D8D8;
}
.oneItemDetail:first-child:before {
  display: none;
}
.itemLabel{
  width: 40%;
}
.itemDetailValue{
  color: #999;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}